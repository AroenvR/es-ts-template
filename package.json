    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.3",
    "@types/node": "^20.2.5",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "eslint": "^8.41.0",
    "eslint-plugin-react": "^7.32.2",
    "jest": "^29.6.2",
    "nodemon": "^3.1.4",
    "ts-jest": "^29.1.1",
    "typescript": "^5.0.4"
